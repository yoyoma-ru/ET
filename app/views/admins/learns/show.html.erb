<div class="container">
	<div class="row">
		<div class="col-sm-offset-1 col-sm-10">
			<h2>管理者用　Learn詳細</h2>
			    <div>
			    	<h4>id</h4>
			    	<%= @learn.id %>
			    	<h4>パート</h4>
			    	<%= link_to edit_admins_learn_path(@learn) do %>
			    	    <%= @learn.part %>
			    	<% end %>
			    	<h3>タイトル</h3>
			    	<%= @learn.title %>
			    	<h3>サブタイトル</h3>
			    	<%= @learn.subtitle %>
			    	<h3>説明文</h3>
			    	<%= @learn.explanation %>
			    	<h3>非エッセンシャル思考例</h3>
			    	<%= @learn.example_non_essential_thinking %>
			    	<h3>エッセンシャル思考例</h3>
			    	<%= @learn.example_essential_thinking %>
			    </div>
			    <%= link_to '編集する', edit_admins_learn_path(@learn), class: "btn btn-primary col-sm-2"%>
			    <span>&emsp;</span>
			    <%= link_to '削除する（削除ができない）', admins_learn_path(@learn), class: "btn btn-danger btn-sm", method: :delete, data: {confirm: "本当に削除してもよろしいですか？"} %>
		</div>
	</div>
</div>